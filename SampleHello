// simplify labels
pipeline {
    agent any
    stages {
        stage('Test') {
            steps {
                echo 'Unit Testing...'
	   	// sh (pipeline step to execute a shell command)
		     

		}


       		 }  	

	stage('QA') {
	   steps {
	          echo 'QA/Staging Environment'
		  }
		  }
	stage('Production'){

	     steps {
	           echo 'Deployed'
		   }

    		}
}
}
